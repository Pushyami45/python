date=input("enter the date to be checked:")
c=date.split("/")
b=list(map(int,c))
year=(b[2])
if(year%4==0):
 if(year%100==0):
  if(year%400==0):
   print("%d is leap year"%year)
  else:
   print(%d is not leap year"%year)
 else:
  print(%d is not leap year"%year)
else:
 print(%d is not leap year"%year)
x=year%4
if(x!=0):
 print("previous leap year : ",year-x)
else:
 print(next leap year : ",year+4)
